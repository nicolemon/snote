[global]
  editor=vim
  ext=md
  datefmt="%Y-%m-%d"
  timefmt="%H:%M:%S"
  template=$HOME/test-snotebooks/template1
  max_list=1

[nb1]
  path=$HOME/test-snotebooks/nb1
  timefmt="%H:%M"
  max_list=2

[nb2]
  path=$HOME/test-snotebooks/nb2
  datefmt="%Y %B %d"
  template=$HOME/test-snotebooks/template2
  ext=html

[nb3]
  path=$HOME/test-snotebooks/nb3
  ext=txt

[badpath]
  path=/path/does/not/exist
